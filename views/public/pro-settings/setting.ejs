<!DOCTYPE html>
<html lang="en">
    <head>
    
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    
       
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <title>PRO SETTINGS</title>
    </head>
<body>
    <main>
        <div class="container py-4">

            <header class="p-3 mb-3 border-bottom">
              <%- include('../../publicHeader'); %>
              </header>
    
    <section class="show">
        <table class="table table-hover table-dark bg-dark">
            <thead>
            <tr>
            <th>Name</th>
            <th>Team</th>
            <th>Mouse</th>
            <th>Hertz</th>
            <th>Dpi</th>
            <th>Sensitivity</th>
            <th>Edpi</th>
            <th>Ads</th>
            <th>Monitor</th>
            <th>Refresh</th>
            <th>Gpu</th>
            <th>Resolution</th>
            <th>Mousepad</th>
            <th>Keyboard</th>
            <th>Headset</th>
            
        </tr>
    </thead>
            <h1>PRO SETTINGS</h1>
        <% settings.forEach(function(setting){ %>
            <tbody>
            <tr class="table-danger"> 
            <td> <h6><%= setting.name %></h6> </td>    
            <td> <h6><%= setting.team %></h6> </td> 
            <td> <h6><%= setting.mouse %></h6> </td> 
            <td> <h6><%= setting.hz %></h6> </td> 
            <td> <h6><%= setting.dpi %></h6> </td> 
            <td> <h6><%= setting.sensitivity %></h6> </td> 
            <td> <h6><%= setting.edpi %></h6> </td> 
            <td> <h6><%= setting.ads %></h6> </td> 
            <td> <h6><%= setting.monitor %></h6> </td> 
            <td> <h6><%= setting.refresh %></h6> </td> 
            <td> <h6><%= setting.gpu %></h6> </td> 
            <td> <h6><%= setting.resolution %></h6> </td> 
            <td> <h6><%= setting.mousepad %></h6> </td> 
            <td> <h6><%= setting.keyboard %></h6> </td> 
            <td> <h6><%= setting.headset %></h6> </td> 
           
            
            
            </tr>
        </tbody>
        <% }) %>
    </table>
    </section>
    <footer class="pt-3 mt-4 text-muted border-top">
        2022 &copy;  ZENO
    </footer>
</div>
</main>



<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
crossorigin="anonymous"></script>
</body>
</html>