<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Leaderboard</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<body>
    <main>
        <div class="container py-4">

            <header class="p-3 mb-3 border-bottom">
                <%- include('../../publicHeader'); %>
                </header>
      
    
    <section class="show">
        <table class="table table-hover table-dark bg-dark mb-3">
            <thead>
            <tr>
                <th>Rank</th>
                <th>Name</th>
                <th>Wins</th>
                <th>RR</th>
            
            
        </tr>
    </thead>
            <h1>Leaderboard</h1>
        <% leaderboards.forEach(function(leaderboard){ %>
            <% for (var i=0; i <=100; i++){ %>
            <tbody>
            <tr class="table-danger"> 
                   
                <td> <h6><%= leaderboard.data.leaderboardRank[i] %></h6> </td> 
                <td> <h6><%= leaderboard.data.gameName[i] %></h6> </td> 
                <td> <h6><%= leaderboard.data.numberOfWins[i] %></h6> </td> 
                <td> <h6><%= leaderboard.data.rankedRating[i] %></h6> </td> 
            
            </tr>
        </tbody>
        <% } %>
        <% }) %>
    </table>

    </section>
  


        </form>
    </section>
    <footer class="pt-3 mt-4 text-muted border-top">
        2022 &copy;  ZENO
    </footer>
</div>
</main>



<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
crossorigin="anonymous"></script>
</body>
</html>