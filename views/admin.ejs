<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Login and Register</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  </head>
  <body>
    <header>
      <%- include('adminHeader'); %>
    </header>
    <center>
    <div class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
      <% users.forEach(function(user){ %>
        <form action="/logout?_method=DELETE" method="POST">
          <button type="submit" class="btn btn-outline-danger"><%= user.name %> LOGOUT </button>
        </form>
        <% }) %>
    </div>

    <span class="badge rounded-pill bg-success">Pages</span><br> <br>

    <div class="card border-warning mb-3" style="max-width: 20rem;">
      <div class="card-body">
        <a href="/">
        <button type="button" class="btn btn-warning">Homepage</button></a>
      </div>
    </div>

    <div class="card border-warning mb-3" style="max-width: 20rem;">
      <div class="card-body">
        <a href="/agents">
        <button type="button" class="btn btn-warning">Agents</button></a>
      </div>
    </div>

    <div class="card border-warning mb-3" style="max-width: 20rem;">
      <div class="card-body">
        <a href="/weapons">
        <button type="button" class="btn btn-warning">Weapons</button></a>
      </div>
    </div>

    <div class="card border-warning mb-3" style="max-width: 20rem;">
      <div class="card-body">
        <a href="/maps">
        <button type="button" class="btn btn-warning">Maps</button></a>
      </div>
    </div>

    <div class="card border-warning mb-3" style="max-width: 20rem;">
      <div class="card-body">
        <a href="/guides">
        <button type="button" class="btn btn-warning">Guides</button></a>
      </div>
    </div>

    <div class="card border-warning mb-3" style="max-width: 20rem;">
      <div class="card-body">
        <a href="/setting">
        <button type="button" class="btn btn-warning">Pro's Settings</button></a>
      </div>
    </div>

    <div class="card border-warning mb-3" style="max-width: 20rem;">
      <div class="card-body">
        <a href="/leaderboard">
        <button type="button" class="btn btn-warning">Leaderboard</button></a>
      </div>
    </div>

    <div class="card border-warning mb-3" style="max-width: 20rem;">
      <div class="card-body">
        <a href="/player">
        <button type="button" class="btn btn-warning">Search Player</button></a>
      </div>
    </div>

  </center>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
crossorigin="anonymous"></script>

  </body>
</html>